<div class="row">
  <div class="find-table-restaurant__card__image">
    <img src="{{imageFileName}}">
  </div>
  <div class="find-table-restaurant__card__information">
    <h6 class="col-12">Restaurant</h6>
    <h5 class="col-12">{{restaurantName}}</h5>
    <div class="find-table-restaurant__card__information__header col-12 form-inline">
      <h6 >Guest</h6>
      <h6>Date</h6>
      <h6>Time</h6>
    </div>
    <div class="find-table-restaurant__card__information__reservation col-12 form-inline">
      <h5>{{persons}} People</h5>
      <h5>{{date}}</h5>
      <h5>{{hours}}</h5>
    </div>
  </div>
</div>

